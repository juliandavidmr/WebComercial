{
  "name": "independiente",
  "version": "1.0.0",
  "description": "API",
  "main": "app.js",
  "scripts": {
    "babel-node": "babel-node --presets=es2015",
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel -d ./build ./app.js -s",
    "build": "npm run clean && npm run build-server",
    "start": "node ./build/index.js",
    "build_routes": "babel routes --out-dir build/routes",
    "build_db": "babel db --out-dir build/db",
    "build_public": "babel public --out-dir build/public",
    "build_app": "babel app.js --out-dir build/"
  },
  "keywords": [
    "api"
  ],
  "author": "yi.molina@udla.edu.co",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "*",
    "body-parser": "^1.15.0",
    "connect-flash": "*",
    "cookie-parser": "^1.4.1",
    "dialog": "^0.2.0",
    "ejs": "^2.4.1",
    "express": "^4.13.4",
    "express-ejs-layouts": "^2.0.0",
    "express-session": "^1.13.0",
    "express-validator": "*",
    "jsonwebtoken": "^7.4.1",
    "knex": "^0.13.0",
    "moment": "^2.12.0",
    "mongoose": "^4.4.12",
    "morgan": "^1.7.0",
    "mysql": "^2.10.2",
    "node-sass": "^4.5.3",
    "node-sass-middleware": "^0.11.0",
    "passport": "*",
    "passport-http": "*",
    "passport-local": "*",
    "socket.io": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.10.4",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.0",
    "gulp-sourcemaps": "^2.6.0"
  }
}
