
<div class="wrapper">
          <div class="main-panel">
              <div class="content">
                  <div class="container-fluid">
                      <div class="row">
                          <div class="col-md-12">
                              <div class="card">
                                <!--<span class="active total" style="display:block;" data-toggle="modal" data-target="#myModal"><strong>All USA</strong> (Select your city to see local ads)</span>-->
                                  <div class="header">
                                    <h1 class="title">Gestionar comerciantes</h1>
                                  </div>
                                  <hr/>
                                  <div class="content">
                                      <form method="post" action="/comerciante/register">
                                          <div class="row">
                                              <div class="col-md-12">
                                                  <div class="form-group">
                                                      <label>Nombre</label>
                                                      <input type="text" class="form-control" name="nombre" required>
                                                  </div>
                                              </div>
                                              <div class="col-md-12">
                                                  <div class="form-group">
                                                      <label>Descripción</label>
                                                      <textarea name="descripcion" cols="40" rows="3" class="form-control" ></textarea>
                                                  </div>
                                              </div>
                                              <div class="col-md-6">
                                                  <div class="form-group">
                                                      <label>Correo</label>
                                                      <input type="text" class="form-control" name="correo" required>
                                                  </div>
                                              </div>
                                              <div class="col-md-6">
                                                  <div class="form-group">
                                                      <label>Página web</label>
                                                      <input type="text" class="form-control" name="pagina_web" required>
                                                  </div>
                                              </div>
                                              <div class="col-md-6">
                                                  <div class="form-group">
                                                      <label>Teléfono</label>
                                                      <input type="text" class="form-control" name="telefono" pattern="[0-9]*" title="Digita sólo números" required>
                                                  </div>
                                              </div>
                                              <div class="col-md-6">
                                                  <div class="form-group">
                                                      <label>Dirección</label>
                                                      <input type="text" class="form-control" name="direccion" required>
                                                  </div>
                                              </div>
                                              <div class="col-md-6">
                                                  <div class="form-group">
                                                      <label>Latitud</label>
                                                      <input type="text" class="form-control" name="latitud">
                                                  </div>
                                              </div>
                                              <div class="col-md-6">
                                                  <div class="form-group">
                                                      <label>Longitud</label>
                                                      <input type="text" class="form-control" name="longitud">
                                                  </div>
                                              </div>
                                          </div>

                                          <button type="submit" class="btn btn-info btn-fill pull-right">Registrar</button>
                                          <div class="clearfix"></div>
                                      </form>
                                  </div>
                              </div>
                          </div>
                      </div>
                  </div>
              </div>
          </div>
      </div>
